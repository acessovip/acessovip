{
  "routes": [
    {
      "src": "/.well-known/(.*)",
      "dest": "/public/.well-known/$1"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
